{
  "cli": {
    "version": ">= 4.1.2"
  },
  "build": {
    "base": {
      "node": "18.19.1",
      "pnpm": "8.5.0",
      "ios": {
        "resourceClass": "m-medium"
      },
      "channel": "base"
    },
   "clip": {
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "channel": "clip"
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "preview"
    },
    "production": {
      "extends": "base",
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "oppfy.app.official@gmail.com",
        "appleTeamId": "UAYFD9QXLS",
        "appName": "Oppfy"
      }
    }
  }
}
