{
  "name": "@oppfy/expo",
  "version": "0.1.0",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "android": "pnpm with-env expo run:android",
    "build:ios": "pnpm with-env expo build:ios",
    "build:ipa": "rm -f *.ipa && pnpm with-env eas build --profile production --local",
    "clean": "git clean -xdf .expo .gitignore .turbo node_modules",
    "dev": "pnpm with-env expo start",
    "dev:android": "pnpm with-env expo start --android",
    "dev:ios": "pnpm with-env expo start --ios",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "generate-themes": "tamagui generate-themes ./theme-builder.ts ./theme-output.ts",
    "ios": "pnpm with-env expo run:ios",
    "lint": "eslint",
    "open:android": "open -a \"Android Studio\" android",
    "open:ios": "open -a \"Xcode\" ios",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "submit": "eas submit --platform ios --path",
    "typecheck": "tsc --noEmit",
    "update:production": "eas update --channel production --message \"$MESSAGE\"",
    "with-env": "dotenv -e ../../.env --"
  },
