{
  "name": "@acme/aws",
  "version": "0.1.0",
  "bin": {
    "aws": "dist/bin/aws.js"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "pnpm with-env cdk",
    "dev": "tsc -w",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "with-env": "dotenv -e ../../.env --",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@acme/db": "workspace:^0.1.0",
    "@aws-sdk/client-s3": "^3.495.0",
    "aws-cdk-lib": "2.116.1",
    "aws-lambda": "^1.0.7",
    "buffer": "^6.0.3",
    "constructs": "^10.0.0",
    "net": "^1.0.2",
    "source-map-support": "^0.5.21",
    "tsx": "^4.7.1"
  },
  "devDependencies": {
    "@acme/eslint-config": "workspace:^0.2.0",
    "@acme/prettier-config": "workspace:^0.1.0",
    "@acme/tailwind-config": "workspace:^0.1.0",
    "@acme/tsconfig": "workspace:^0.1.0",
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^29.5.11",
    "@types/node": "^18.16.5",
    "aws-cdk": "2.116.1",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@acme/eslint-config/base"
    ]
  },
  "prettier": "@acme/prettier-config"
}
