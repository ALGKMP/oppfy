    "@acme/tailwind-config": "workspace:^0.1.0",
    "@acme/tsconfig": "workspace:^0.1.0",
    "@types/aws-lambda": "^8.10.129",
    "@serverless/typescript": "^3.38.0",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.50.0",
    "serverless-offline": "^13.3.2",
    "ts-node": "^10.9.2",
    "json-schema-to-ts": "^2.9.2",
    "eslint": "^8.53.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
